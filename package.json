{
  "name": "gozer",
  "version": "0.0.5",
  "description": "A phantomJS wrapper for testing your CSS",
  "author": "Ryland Herrick <a@rylnd.com>",
  "contributors": [
    "Kevin Altman <kevin@itsthatguy.com>"
  ],
  "keywords": [
    "css", "html", "integration", "phantom", "phantomjs",
    "spec", "specs", "test", "testing", "tests", "unit"
  ],
  "main": "lib/gozer",
  "scripts": {
    "build": "node_modules/.bin/coffee --bare --compile --output lib/gozer src/*.coffee",
    "prepublish": "node_modules/.bin/coffee --bare --compile --output lib/gozer src/*.coffee",
    "postpublish": "rm -rf lib",
    "test": "node_modules/.bin/mocha"
  },
  "engines": {
    "node": "0.10.x"
  },
  "dependencies": {
    "phantom": "^0.6.5",
    "phantomjs": "^1.9.7-15",
    "q": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.1",
    "coffee-script": "^1.7.1",
    "express": "^4.0.0",
    "mocha": "^1.20.1"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/adorableio/gozer.git"
  },
  "bugs": {
    "url": "https://github.com/adorableio/gozer/issues"
  }
}
